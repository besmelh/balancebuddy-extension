{
    "name": "Getting Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "host_permissions": ["http://localhost:5000/*"],
    "permissions": ["http://localhost:5000/"],
    "action": {
        "default_popup": "popup.html"
      },
      "background": {
          "page": "background.html",
          "persistent": "false"
      }
  }